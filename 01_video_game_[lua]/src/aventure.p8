--map

function draw_map()
	map(0,0,0,0,128,64)
end

function update_camera()
	camx=flr(p.x/128)*128
	camy=flr(p.y/128)*128
	camera(camx,0)
end

-->8
--player

function create_player()
	p={
		x=23,
		y=80,
		w=8,
		h=8,
		sprite=142,
		flip_x=false,
		flip_y=false,
		dx=0,
		dy=0,
		has_key=false,
		has_treasure=false
	}
end

function player_update()
	p.dx=0
	if (btn(➡️)) then
	 p.dx=1
	 p.sprite=142
			if p.x>239 then p.x=239
			end
	end
	if (btn(⬅️)) then
	 p.dx=-1
	 p.sprite=143
		 if p.x<1 then p.x=1
		 end
	end
	
	if is_grounded() then
		p.dy=0
  if (btnp(⬆️)) then 
  	p.dy=-6
 	end
 else
	 p.dy+=1
	 if (p.dy>4) then
	 	p.dy=4
	 end 
	 	if p.y>125 then p.y=125
			end
 end
 
 if is_drowning() then
 	p.dy+=0
 	p.sprite=158
 	 if (p.dy>2.5) then
	 	p.dy=2.5
		 end
 end

	
	if p.dx==1 and there_is_a_wall() then
		p.dx=0
	end
	
 p.y+=p.dy
 p.x+=p.dx
 
	if is_grounded() then 
		 p.y=flr(p.y/8)*8
	end
	
	if flr(p.x/8)==29
	 and flr(p.y/8)==8	then
			p.has_key=true
	end
	
	if flr(p.x/8)==30
		and flr(p.y/8)==4 then
			  p.has_treasure=true
	end 
end

function draw_player()
	spr(p.sprite,p.x,p.y,
					1,1,p.flip_x,p.flip_y)
end
-->8
--collisions

function is_grounded()
 local	bloc = mget(flr(p.x+4)/8,
												  flr(p.y)/8+1)
	return fget(bloc,0)
end
 
function is_drowning()
 local	bloc = mget(flr(p.x+4)/8,
												  flr(p.y)/8+1)
	return fget(bloc,1)	
end

function there_is_a_wall()
 local	bloc = mget(flr(p.x/8+1),
												 	flr(p.y)/8)
	return fget(bloc,0)	
end

