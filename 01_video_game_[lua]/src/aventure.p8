--key

function create_key(x,y)
	k={
		x=x,
		y=y,
		w=8,
		h=8,
		sprite=23
		}
end

function update_key()
	if p.has_key then
		k.sprite=24
	end
end

function draw_key()
		spr(k.sprite,k.x,k.y,
					k.w/8,k.h/8,false)
end
-->8
--treasure

function create_treasure(x,y)
	t={
		x=x,
		y=y,
		w=8,
		h=8,
		sprite=25
		}
end

function update_treasure()
	if p.has_treasure
	and p.has_key
	 then
		t.sprite=24
-- return true
	end
end

function draw_treasure()
		spr(t.sprite,t.x,t.y,
					t.w/8,t.h/8,false)
end
